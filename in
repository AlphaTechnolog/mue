struct Rectangle {
  double y
  double x
  double width
  double height
}

struct Vec2d {
  double x
  double y
}

struct Color {
  double r
  double g
  double b
  double a
}

void DrawRectangle(Vec2d *pos, Rectangle *size, Color *color); // js_draw_rectangle
void InitWindow(int width, int height, char *title); // js_init_window
